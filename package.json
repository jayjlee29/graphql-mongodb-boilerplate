{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "mongodb": "docker run --rm --name local-mongo -p 27017:27017 -d mongo",
    "start": "node ./dist/server.js",
    "dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
