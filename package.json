    "graphql-compose": "^7.22.1",
    "graphql-compose-mongoose": "^9.0.0",
    "graphql-subscriptions": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "restify": "^8.5.1",
    "subscriptions-transport-ws": "^0.9.18"
